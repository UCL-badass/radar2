Model CVB;
Objective Max ENB = EV(NB);
Objective Min LP=percentile(-NB,1);
//Objective Min LP=Pr(NB <0);
NB = Benefit - Cost;
Benefit = decision("system type"){
	"New" 	: normal_ci(1, 9);
	"Legacy" : normal_ci(0.9, 1.1);
}
Cost = decision("system type"){
	"New" : normal_ci(1, 5);
	"Legacy" : normal_ci(0, 0);
}
