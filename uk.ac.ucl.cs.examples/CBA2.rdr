Model CVB;
Objective Max ENB = E(NB);
Objective Min LP=P(NB <0);
NB = Benefit - Cost;
Benefit = decision("system type"){
	"New" 	: normal_ci(1, 9);
	"Legacy" : normal_ci(0.9, 1.1);
}
Cost = decision("system type"){
	"New" : A;
	"Legacy" : A + B;
}
A =normal_ci(1, 5);
B = C * D;
C =normal_ci(0, 0);
D =deterministic (1/3);